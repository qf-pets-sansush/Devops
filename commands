   16  if [ $# -lt 1 ]; then    echo "USAGE: $0 [-daemon] server.properties [--override property=value]*";      exit 1; fi
   17  base_dir=$(dirname $0)
   18  if [ "x$KAFKA_LOG4J_OPTS" = "x" ]; then     export KAFKA_LOG4J_OPTS="-Dlog4j.configuration=file:$base_dir/../config/log4j.properties"; fi
   19  if [ "x$KAFKA_HEAP_OPTS" = "x" ]; then     export KAFKA_HEAP_OPTS="-Xmx1G -Xms1G"; fi
   20  EXTRA_ARGS=${EXTRA_ARGS-'-name kafkaServer -loggc'}
   21  COMMAND=$1
   22  case $COMMAND in   -daemon)     EXTRA_ARGS="-daemon "$EXTRA_ARGS;     shift;     ;;   *)     ;; esac
   23  exec $base_dir/kafka-run-class.sh $EXTRA_ARGS kafka.Kafka "$@"
